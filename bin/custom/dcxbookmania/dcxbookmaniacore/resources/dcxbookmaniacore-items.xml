		</enumtype>
		<enumtype generate="true" code="CountryEnum"
			autocreate="true" dynamic="true">
			<value code="USA" />
			<value code="UK" />
			<value code="CAN" />
			<value code="FRA" />
			<value code="AUS" />
			<value code="PORT" />
