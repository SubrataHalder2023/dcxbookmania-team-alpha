			<value code="RED" />
			<value code="SILVER" />
			<value code="WHITE" />
			<value code="YELLOW" />
		</enumtype>
		<enumtype generate="true" code="EditionEnum"
			autocreate="true" dynamic="true">
			<value code="HARDCOVER" />
			<value code="PAPERBACK" />
			<value code="EBOOK" />
			<value code="AUDIOBOOK" />
			<value code="LIMITED" />
			<value code="REVISED" />
