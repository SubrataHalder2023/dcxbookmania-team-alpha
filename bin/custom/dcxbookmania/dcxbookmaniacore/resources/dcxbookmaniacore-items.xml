	<maptypes>
		<maptype code="KeywordMap"
                 argumenttype="java.lang.String"
                 returntype="java.lang.Integer"
                 autocreate="true"
                 generate="false"/>
	</maptypes>
	
	<relations>
		<relation code="Book2Merchandise" generate="true" localized="false" autocreate="true">
			<deployment table="Book2Merchandise" typecode="11002"/>
            <sourceElement qualifier="products" type="DcxbookmaniaProduct" cardinality="many" ordered="false">
                <modifiers read="true" write="true" search="true" optional="true"/>
            </sourceElement>
            <targetElement qualifier="merchandises" type="DcxbookmaniaMerchandise" cardinality="many" collectiontype="list" ordered="true">
                <modifiers read="true" write="true" search="true" optional="true"/>
            </targetElement>  
        </relation>
	</relations>
	
	
	<itemtypes>

